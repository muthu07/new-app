{
  "name": "my-first-project",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build --extra-webpack-config webpack.extra.config.js --base-href ./",
    "build:prod": "ng build --configuration=production --extra-webpack-config webpack.extra.config.js --base-href ./",
    "build:stats": "npm run build:prod -- --stats-json && webpack-bundle-analyzer dist/ace-app/stats-es2015.json",
    "ci": "npm run lint && npm run test:ci && npm run build:prod",
    "generate": "ng generate",
    "lint:fix": "npm run lint:ts -- --fix && npm run lint:scss -- --fix",
    "lint:format": "prettier --write \"{src,e2e}/**/*.{js,ts,html,scss}\"",
    "lint:scss": "stylelint \"{src,e2e}/**/*.scss\"",
    "lint:ts": "eslint \"{src,e2e}/**/*.{js,ts}\" --max-warnings=520",
    "lint": "npm run lint:ts && npm run lint:scss",
    "docs": "compodoc -s tsconfig.app.json",
    "docs:build": "compodoc -p tsconfig.app.json",
    "start": "ng serve --open",
    "start:test": "ng serve --configuration=test --open",
    "start:local": "ng serve --configuration=local --open",
    "start:uat": "ng serve --configuration=uat --open",
    "start:prod": "ng serve --configuration=prod --open",
    "test": "ng test",
    "test:ci": "npm run test:coverage",
    "test:coverage": "ng test --watch=false --browsers=Headless --codeCoverage=true",
    "test:e2e": "ng e2e",
    "test:e2e:dev": "npm run test:e2e -- --configuration=dev",
    "test:e2e:sit": "npm run test:e2e -- --configuration=sit",
    "test:e2e:uat": "npm run test:e2e -- --configuration=uat",
    "test:e2e:debug": "npm start & node --inspect-brk ./node_modules/.bin/protractor e2e/protractor.conf.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run ci",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,e2e}/**/*.{js,ts}": [
      "eslint --fix"
    ],
    "{src,e2e}/**/*.scss": [
      "stylelint --fix"
    ],
    "{src,e2e}/**/*.{html,json}": [
      "prettier --write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/platform-server": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@angular/service-worker": "~9.1.9",
    "@ng-select/ng-select": "^3.7.0",
    "@ngrx/effects": "8.2.0",
    "@ngrx/entity": "8.2.0",
    "@ngrx/store": "8.2.0",
    "@ngrx/store-devtools": "8.2.0",
    "chart.js": "2.8.0",
    "jwt-decode": "^2.2.0",
    "lodash-es": "^4.17.15",
    "moment": "2.24.0",
    "ng2-charts": "^2.3.0",
    "ngx-quill": "7.0.2",
    "quill": "^1.3.7",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "~8.0.1",
    "@compodoc/compodoc": "^1.1.11",
    "@ngrx/schematics": "8.2.0",
    "@testing-library/angular": "^8.2.0",
    "@types/chai": "^4.2.7",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cucumber": "^6.0.0",
    "@types/jasmine": "^3.5.1",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.1.8",
    "@types/protractor": "^4.0.0",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codelyzer": "^5.1.2",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.1.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-cucumber": "^1.4.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-testing-library": "^2.2.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^10.0.8",
    "ngx-build-plus": "^9.0.2",
    "ngx-xml2json": "^1.0.2",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "protractor": "~7.0.0",
    "protractor-cucumber-framework": "^6.2.0",
    "standard-version": "^7.1.0",
    "stylelint": "^13.2.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-group-selectors": "^1.0.7",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.14.2",
    "stylelint-use-nesting": "^2.0.0",
    "ts-node": "~8.3.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~6.1.0",
    "typescript": "^3.8.3",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
